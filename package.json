    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "babel-core": "^4.7.8",
    "babel-loader": "^4.1.0",
    "bundle-loader": "^0.5.0",
    "chai": "^2.1.0",
    "chai-as-promised": "^4.1.0",
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
